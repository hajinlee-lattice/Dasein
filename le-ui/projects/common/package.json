    "type": "svn",
    "url": "https://lesvn.lattice.local/svn/lattice/LEDP/branches/develop/ledp/le-ui"
  },
  "dependencies": {
    "@uirouter/angularjs": "~1.0.18",
    "@uirouter/react": "^0.8.7",
    "@uirouter/react-hybrid": "^0.3.4",
    "angular": "~1.5.8",
    "angulartics": "^1.6.0",
    "angulartics-mixpanel": "^0.1.4",
    "axios": "^0.18.0",
    "bootstrap3": "^3.3.5",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^2.0.0",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^1.0.0",
    "d3-3": "0.0.0",
    "dateformat": "^3.0.3",
    "eslint": "^5.14.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "jstorage": "^0.4.8",
    "lodash": "^4.17.11",
    "ng-redux": "4.1.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.9.1",
    "pako": "^1.0.6",
    "prop-types": "^15.6.2",
    "raw-loader": "^0.5.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-transition-group": "^1.2.1",
    "react2angular": "^4.0.4",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "rxjs": "^6.3.3",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
