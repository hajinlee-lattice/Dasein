{
  "name": "LE-UI_Common",
  "description": "shared libraries and resources for le-ui web apps",
  "version": "1.0.0",
  "license": "ISC",
  "repository": {
    "type": "svn",
    "url": "https://lesvn.lattice.local/svn/lattice/LEDP/branches/develop/ledp/le-ui"
  },
  "dependencies": {
    "@uirouter/angularjs": "~1.0.18",
    "@uirouter/react": "^0.8.7",
    "@uirouter/react-hybrid": "^0.3.4",
    "angular": "~1.5.8",
    "angulartics": "^1.6.0",
    "angulartics-mixpanel": "^0.1.4",
    "axios": "^0.18.0",
    "bootstrap3": "^3.3.5",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^2.0.0",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^1.0.0",
    "d3-3": "0.0.0",
    "dateformat": "^3.0.3",
    "eslint": "^3.17.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "jstorage": "^0.4.8",
    "lodash": "^4.17.10",
    "ng-annotate-loader": "^0.6.1",
    "node-sass": "^4.8.3",
    "nodemon": "^1.9.1",
    "pako": "^1.0.6",
    "prop-types": "^15.6.2",
    "raw-loader": "^0.5.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react2angular": "^4.0.4",
    "rxjs": "^6.3.3",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "underscore": "^1.9.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "expose-loader": "^0.7.5",
    "grunt": "^0.4.5",
    "grunt-concurrent": "^2.2.1",
    "grunt-contrib-concat": "^1.0.0",
    "grunt-contrib-sass": "^0.9.2",
    "grunt-contrib-uglify": "^0.11.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-ng-annotate": "^2.0.1",
    "jest": "23.6.0",
    "jest-enzyme": "^7.0.1",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "bower_components",
      "node_components"
    ],
    "modulePaths": [
      "/shared/vendor/modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "setupTestFrameworkScriptFile": "jest-enzyme",
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "moduleNameMapper": {
      "^common(.*)$": "<rootDir>/$1",
      "^widgets(.*)$": "<rootDir>/widgets/$1",
      "^config$": "<rootDir>/configs/app-config.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "scripts": {
    "test": "jest --coverage",
    "dev-test": "jest --watchAll --colors",
    "build": "webpack --mode=production",
    "watch": "webpack --mode=development --watch"
  }
}
