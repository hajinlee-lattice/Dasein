<div class="analytic-attribute-tile border-top">
    <div class="float-right">
        <% if (options.Lift != null) {%>
            <% if (options.ShowLift == true) {%>
                <div class="lift-container">
                    <div class="lift <% if(options.Lift > 1) { %> pos <% } %> <% if(options.Lift < 1) { %> neg <% } %>"><%= options.Lift %></div>
                    <div class="lift-sub-label"><%= ResourceUtil.getString('LIFT_SUB_LABEL') %></div>
                </div>
                
            <% } else { %>
                    <% if(options.Lift > 1) { %>
                        <img class="lift-image" src="assets/images/lift-up-arrow.png"/>
                        <div class="lift-image-description"><%= ResourceUtil.getString('LIFT_ABOVE_AVERAGE') %></div>
                    <% } else { %>
                        <img class="lift-image" src="assets/images/lift-down-arrow.png"/>
                        <div class="lift-image-description"><%= ResourceUtil.getString('LIFT_BELOW_AVERAGE') %></div>
                    <% } %>
                </span>
            <% } %>
        <% } %>
    </div>
  
    <div class="analytic-attribute-heading"><%= options.DisplayName %></div>
    <div class="analytic-attribute-title"><%= options.BucketName %></div>
    
    <% if (options.Description != null && options.Description != "") {%>
        <div class="analytic-attribute-description">
            <%= options.Description %>
        </div>
    <% } %>
</div>