pipeline {
  agent {
    node {
      label "general"
    }
  }
  stages {
    stage('build') {
      steps {
        script {
          jobs = [:]
          jobs['cdl'] = {
            build job: '../trunk_health_cdl'
          }
          jobs['lpi'] = {
            build job: '../trunk_health_lpi'
          }
          parallel jobs
        }
      }
    }
  }
}
