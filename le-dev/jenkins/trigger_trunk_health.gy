pipeline {
  agent {
    node {
      label "general"
    }
  }
  stages {
    stage('build') {
      steps {
        jobs = [:]
        jobs['cdl'] = {
          build job: 'cd_trunk_health_cdl'
        }
        jobs['lpi'] = {
          build job: 'cd_trunk_health_lpi'
        }
        parallel jobs
      }
    }
  }
}
