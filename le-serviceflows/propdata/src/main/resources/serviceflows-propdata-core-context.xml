<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
          http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">

    <context:component-scan base-package="com.latticeengines.propdata.core"/>

    <import resource="classpath:serviceflows-propdata-persistence-context.xml"/>
    <import resource="classpath:serviceflows-propdata-aspects-context.xml"/>
    <import resource="classpath:dataplatform-context.xml"/>
    <import resource="classpath:camille-runtime-context.xml"/>
    <import resource="classpath:proxy-context.xml"/>
    <import resource="classpath:security-no-controller-context.xml"/>
    <import resource="classpath:common-threadpool-context.xml"/>

    <bean id="dataCloudVersionDao" class="com.latticeengines.propdata.core.dao.impl.DataCloudVersionDaoImpl">
        <property name="sessionFactory" ref="sessionFactoryPropDataManage"/>
    </bean>

</beans>