<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
          http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">

    <context:component-scan base-package="com.latticeengines.propdata.match" />

    <import resource="classpath:propdata-core-context.xml"/>
    <import resource="classpath:serviceflows-propdata-match-context.xml"/>
    <import resource="classpath:propdata-yarn-client-context.xml"/>
    <import resource="classpath:propdata-persistence-match-context.xml"/>
    <import resource="classpath:dataplatform-context.xml" />
    <import resource="classpath:dataflow-context.xml"/>
    <import resource="classpath:security-no-controller-context.xml"/>
    <import resource="classpath:monitor-metric-context.xml" />
    <import resource="classpath:propdata-match-task-context.xml" />

    <bean id="externalColumnDao" class="com.latticeengines.propdata.match.dao.impl.ExternalColumnDaoImpl">
        <property name="sessionFactory" ref="sessionFactoryPropDataManage"/>
    </bean>

    <bean id="columnMappingDao" class="com.latticeengines.propdata.match.dao.impl.ColumnMappingDaoImpl">
        <property name="sessionFactory" ref="sessionFactoryPropDataManage"/>
    </bean>

</beans>