<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
          http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">

    <context:component-scan base-package="com.latticeengines.propdata.collection"/>

    <import resource="classpath:propdata-core-context.xml"/>
    <import resource="classpath:serviceflows-propdata-collection-context.xml"/>
    <import resource="classpath:dataflow-context.xml"/>

    <bean id="sourceColumnDao" class="com.latticeengines.propdata.collection.dao.impl.SourceColumnDaoImpl">
        <property name="sessionFactory" ref="sessionFactoryPropDataManage"/>
    </bean>

    <bean id="archiveProgressDao" class="com.latticeengines.propdata.collection.dao.impl.ArchiveProgressDaoImpl">
        <property name="sessionFactory" ref="sessionFactoryPropDataManage"/>
    </bean>

    <bean id="refreshProgressDao" class="com.latticeengines.propdata.collection.dao.impl.RefreshProgressDaoImpl">
        <property name="sessionFactory" ref="sessionFactoryPropDataManage"/>
    </bean>

</beans>