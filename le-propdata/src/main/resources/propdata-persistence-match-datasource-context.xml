<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

    <bean id="propDataMatchDataSource" class="com.latticeengines.propdata.match.datasource.MatchClientRoutingDataSource">
        <property name="targetDataSources">
            <map key-type="com.latticeengines.domain.exposed.propdata.MatchClient">
                <entry key="PD126" value-ref="pd126ClientDataSource" />
                <entry key="PD127" value-ref="pd127ClientDataSource" />
                <entry key="PD128" value-ref="pd128ClientDataSource" />
                <entry key="PD144" value-ref="pd144ClientDataSource" />
                <entry key="PD130" value-ref="pd130ClientDataSource" />
                <entry key="PD238" value-ref="pd238ClientDataSource" />
            </map>
        </property>
        <property name="defaultTargetDataSource" ref="pd130ClientDataSource" />
    </bean>

    <bean id="pd126ClientDataSource" class="org.springframework.jdbc.datasource.DriverManagerDataSource">
        <property name="driverClassName" value="com.microsoft.sqlserver.jdbc.SQLServerDriver" />
        <property name="url" value="jdbc:sqlserver://10.51.15.126:1433;databaseName=PropDataMatchDB;" />
        <property name="username" value="${propdata.user}" />
        <property name="password" value="${propdata.password.encrypted}" />
    </bean>

    <bean id="pd127ClientDataSource" class="org.springframework.jdbc.datasource.DriverManagerDataSource">
        <property name="driverClassName" value="com.microsoft.sqlserver.jdbc.SQLServerDriver" />
        <property name="url" value="jdbc:sqlserver://10.51.15.127:1433;databaseName=PropDataMatchDB;" />
        <property name="username" value="${propdata.user}" />
        <property name="password" value="${propdata.password.encrypted}" />
    </bean>

    <bean id="pd128ClientDataSource" class="org.springframework.jdbc.datasource.DriverManagerDataSource">
        <property name="driverClassName" value="com.microsoft.sqlserver.jdbc.SQLServerDriver" />
        <property name="url" value="jdbc:sqlserver://10.51.15.128:1433;databaseName=PropDataMatchDB;" />
        <property name="username" value="${propdata.user}" />
        <property name="password" value="${propdata.password.encrypted}" />
    </bean>

    <bean id="pd144ClientDataSource" class="org.springframework.jdbc.datasource.DriverManagerDataSource">
        <property name="driverClassName" value="com.microsoft.sqlserver.jdbc.SQLServerDriver" />
        <property name="url" value="jdbc:sqlserver://10.51.15.144:1433;databaseName=PropDataMatchDB;" />
        <property name="username" value="${propdata.user}" />
        <property name="password" value="${propdata.password.encrypted}" />
    </bean>

    <bean id="pd130ClientDataSource" class="org.springframework.jdbc.datasource.DriverManagerDataSource">
        <property name="driverClassName" value="com.microsoft.sqlserver.jdbc.SQLServerDriver" />
        <property name="url" value="jdbc:sqlserver://10.51.15.130:1433;databaseName=PropDataMatchDB;" />
        <property name="username" value="${propdata.user}" />
        <property name="password" value="${propdata.password.encrypted}" />
    </bean>

    <bean id="pd238ClientDataSource" class="org.springframework.jdbc.datasource.DriverManagerDataSource">
        <property name="driverClassName" value="com.microsoft.sqlserver.jdbc.SQLServerDriver" />
        <property name="url"
            value="jdbc:sqlserver://10.41.1.238\\SQL2012:1437;databaseName=PropDataMatchDB;" />
        <property name="username" value="${propdata.user}" />
        <property name="password" value="${propdata.password.encrypted}" />
    </bean>

</beans>