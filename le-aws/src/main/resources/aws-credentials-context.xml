<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

    <bean name="awsCredentials" class="com.amazonaws.auth.BasicAWSCredentials">
        <constructor-arg index="0" value="${aws.default.access.key}"/>
        <constructor-arg index="1" value="${aws.default.secret.key.encrypted}"/>
    </bean>

    <bean name="customerCredentials" class="com.amazonaws.auth.BasicAWSCredentials">
        <constructor-arg index="0" value="${aws.customer.access.key}"/>
        <constructor-arg index="1" value="${aws.customer.secret.key.encrypted}"/>
    </bean>

	<bean name="etlCredentials" class="com.amazonaws.auth.BasicAWSCredentials">
        <constructor-arg index="0" value="${aws.etl.access.key}"/>
        <constructor-arg index="1" value="${aws.etl.secret.key.encrypted}"/>
    </bean>
</beans>
