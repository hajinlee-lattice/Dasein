<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
  http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">

    <context:component-scan base-package="com.latticeengines.datacloud.madison"/>

    <import resource="classpath:datacloud-etl-madison-quartz-${PROPDATA_MADISON_QUARTZ_ENABLED:disabled}-context.xml"/>
    <import resource="classpath:datacloud-etl-madison-persistence-context.xml"/>
    <import resource="classpath:datacloud-etl-context.xml"/>
    <import resource="classpath:datacloud-dataflow-context.xml"/>
    <import resource="classpath:yarn-context.xml"/>
    <import resource="classpath:dataflow-context.xml"/>

</beans>