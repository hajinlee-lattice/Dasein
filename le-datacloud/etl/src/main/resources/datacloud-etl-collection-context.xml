<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:component-scan base-package="com.latticeengines.datacloud.collection" />

    <import resource="classpath:datacloud-etl-context.xml" />
    <import resource="classpath:datacloud-dataflow-context.xml" />

    <bean id="archiveProgressDao" class="com.latticeengines.datacloud.collection.dao.impl.ArchiveProgressDaoImpl">
        <property name="sessionFactory" ref="sessionFactoryPropDataManage"/>
    </bean>

    <bean id="refreshProgressDao" class="com.latticeengines.datacloud.collection.dao.impl.RefreshProgressDaoImpl">
        <property name="sessionFactory" ref="sessionFactoryPropDataManage"/>
    </bean>

</beans>