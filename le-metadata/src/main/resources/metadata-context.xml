<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
  http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">

    <context:component-scan base-package="com.latticeengines.metadata">
        <context:exclude-filter type="aspectj" expression="com.latticeengines.metadata.controller.*"/>
        <context:exclude-filter type="aspectj" expression="com.latticeengines.metadata.hive.*"/>
    </context:component-scan>

    <bean id="batonService" class="com.latticeengines.baton.exposed.service.impl.BatonServiceImpl"/>

    <import resource="classpath:monitor-alerts-context.xml"/>
    <import resource="classpath:common-properties-context.xml"/>
    <import resource="classpath:metadata-aspects-context.xml"/>
    <import resource="classpath:metadata-hive-context.xml"/>
    <import resource="classpath:camille-runtime-context.xml"/>
    <import resource="classpath:security-no-controller-context.xml"/>
    <import resource="classpath:db-context.xml"/>
    <import resource="classpath:dataplatform-yarn-config-context.xml"/>
    <import resource="classpath:proxy-context.xml"/>
    <import resource="classpath:dependencyclient-context.xml"/>
</beans>
