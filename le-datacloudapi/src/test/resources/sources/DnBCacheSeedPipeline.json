{
  "Submitter": "RestScanner",
  "Name": "DnBCacheSeedPipeline",
  "Steps": [
    {
      "Transformer": "standardizationTransformer",
      "BaseSources": [
        "DnBCacheSeedRaw"
      ],
      "TargetSource": "DnBCacheSeed",
      "Configuration": {
        "FilterExpression": "DUNS_NUMBER != null",
        "FilterFields": [
          "DUNS_NUMBER"
        ],
        "DomainFields": [
          "LE_DOMAIN"
        ],
        "ConsolidateIndustryStrategy": "PARSE_NAICS",
        "AddConsolidatedIndustryField": "LE_PRIMARY_INDUSTRY",
        "NaicsField": "LE_NAICS_CODE",
        "NaicsMapFileName": "NaicsIndustryMapping.txt",
        "DedupFields": [
          "DUNS_NUMBER",
          "LE_DOMAIN"
        ],
        "ConvertTypeFields": [
          "LE_NUMBER_OF_LOCATIONS",
          "EMPLOYEES_HERE",
          "EMPLOYEES_TOTAL",
          "NUMBER_OF_FAMILY_MEMBERS",
          "SALES_VOLUME_LOCAL_CURRENCY",
          "SALES_VOLUME_US_DOLLARS"
        ],
        "ConvertTypeStrategies": [
          "STRING_TO_INT",
          "STRING_TO_INT",
          "STRING_TO_INT",
          "STRING_TO_INT",
          "STRING_TO_LONG",
          "STRING_TO_LONG"
        ],
        "UploadTimestampField": "LE_Last_Upload_Date",
        "Sequence": [
          "FILTER",
          "DOMAIN",
          "CONSOLIDATE_INDUSTRY",
          "CONVERT_TYPE",
          "DEDUP",
          "UPLOAD_TIMESTAMP"
        ]
      }
    }
  ]
}