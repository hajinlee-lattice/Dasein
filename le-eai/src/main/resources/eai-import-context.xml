<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
      http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:component-scan base-package="com.latticeengines.eai.routes" />
    <context:component-scan base-package="com.latticeengines.eai.entitymanager" />
    <context:component-scan base-package="com.latticeengines.eai.dao" />
    <import resource="classpath:security-no-controller-context.xml"/>
    <import resource="classpath:proxy-context.xml"/>
    <import resource="classpath:eai-mapreduce-context.xml"/>
    <import resource="classpath:redis-context.xml"/>

    <import resource="classpath:aws-context.xml" />
    <import resource="classpath:redshiftdb-context.xml" />
</beans>