<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">


    <bean id="versionManager"
        class="com.latticeengines.common.exposed.version.VersionManager">
        <constructor-arg index="0"
            value="${dataplatform.version:4.17.0-SNAPSHOT}" />
    </bean>

    <bean id="docVersionManager"
        class="com.latticeengines.common.exposed.version.VersionManager">
        <constructor-arg index="0" value="4.17.0-SNAPSHOT" />
    </bean>

    <bean id="beanValidationService"
        class="com.latticeengines.common.exposed.validator.impl.BeanValidationServiceImpl">
    </bean>

</beans>
