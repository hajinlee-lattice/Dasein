<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <artifactId>le-remote</artifactId>
    <packaging>jar</packaging>
    <name>le-${component-name}</name>

    <properties>
        <component-name>remote</component-name>
        <le.common.version>4.5.0-SNAPSHOT</le.common.version>
        <le.domain.version>4.5.0-SNAPSHOT</le.domain.version>
        <le.camille.version>4.5.0-SNAPSHOT</le.camille.version>
        <le.security.version>4.5.0-SNAPSHOT</le.security.version>
    </properties>

    <parent>
        <groupId>com.latticeengines</groupId>
        <artifactId>le-parent</artifactId>
        <version>4.5.0-SNAPSHOT</version>
        <relativePath>../le-parent</relativePath>
    </parent>

    <dependencies>
        <dependency>
            <groupId>com.latticeengines</groupId>
            <artifactId>le-common-web</artifactId>
            <version>${le.common.version}</version>
        </dependency>
        <dependency>
            <groupId>com.latticeengines</groupId>
            <artifactId>le-domain</artifactId>
            <version>${le.domain.version}</version>
        </dependency>
        <dependency>
            <groupId>com.latticeengines</groupId>
            <artifactId>le-camille</artifactId>
            <version>${le.camille.version}</version>
            <classifier>intf</classifier>
        </dependency>
        <dependency>
            <groupId>com.marketo</groupId>
            <artifactId>marketo-soap-sdk</artifactId>
            <version>${marketo.soap.sdk.version}</version>
        </dependency>
        <dependency>
            <groupId>com.latticeengines</groupId>
            <artifactId>le-security</artifactId>
            <version>${le.security.version}</version>
        </dependency>
    </dependencies>

</project>
